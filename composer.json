{
    "name": "thovex-ltd/laravel-postgis",
    "description": "PostGIS extensions for Laravel. Aims to make it easy to work with geometries from Laravel models",
    "require": {
        "php": ">=7.1",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "geo-io/wkb-parser": "^1.0",
        "jmikola/geojson": "^1.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "illuminate/pagination": "^9.0|^10.0|^11.0",
        "phpunit/phpunit": ">=8.5.23",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ThovexLtd\\LaravelPostgis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThovexLtd\\LaravelPostgis\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thovex Ltd."
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ThovexLtd\\LaravelPostgis\\DatabaseServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
